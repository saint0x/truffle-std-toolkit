# API Keys
SERPER_API_KEY=your_serperdev_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Configuration
SEARCH_RESULT_LIMIT=10  # Maximum number of search results to return
SEARCH_TIMEOUT=30       # Search request timeout in seconds

# Browser Configuration
BROWSER_TYPE=chromium   # chromium, firefox, or webkit
BROWSER_TIMEOUT=30000   # Browser operation timeout in milliseconds
BROWSER_HEADLESS=true   # Run browser in headless mode
BROWSER_SCREENSHOTS_DIR=./screenshots  # Directory to save screenshots

# TTS Configuration
TTS_OUTPUT_DIR=./audio_output  # Directory for saving generated audio files
TTS_DEFAULT_VOICE=Josh        # Default ElevenLabs voice ID
TTS_MODEL=eleven_multilingual_v2  # ElevenLabs model ID
TTS_STABILITY=0.5            # Voice stability (0.0-1.0)
TTS_SIMILARITY=0.75          # Voice similarity boost (0.0-1.0) 