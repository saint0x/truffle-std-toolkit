# API Keys
SERPER_API_KEY=your_serperdev_api_key_here
REPLICATE_API_TOKEN=your_replicate_api_token_here

# X (Twitter) Configuration
X_API_KEY=your_x_api_key_here
X_API_SECRET=your_x_api_secret_here
X_ACCESS_TOKEN=your_x_access_token_here
X_ACCESS_TOKEN_SECRET=your_x_access_token_secret_here

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_DEFAULT_FROM=your_twilio_phone_number_here  # Optional: Default sender phone number
TWILIO_MESSAGING_SERVICE_SID=your_messaging_service_sid_here  # Optional: Required if not using default_from

# Configuration
SEARCH_RESULT_LIMIT=10  # Maximum number of search results to return
SEARCH_TIMEOUT=30       # Search request timeout in seconds

# Browser Configuration
BROWSER_TYPE=chromium   # chromium, firefox, or webkit
BROWSER_TIMEOUT=30000   # Browser operation timeout in milliseconds
BROWSER_HEADLESS=true   # Run browser in headless mode
BROWSER_SCREENSHOTS_DIR=./screenshots  # Directory to save screenshots

# Text-to-Speech (TTS) Configuration
TTS_OUTPUT_DIR=./tts_output  # Directory for saving generated audio files
TTS_DEFAULT_VOICE=v2/en_speaker_6  # Default Kokoro voice preset
TTS_MODEL_VERSION=jaaari/kokoro-82m  # Kokoro model version

# Speech-to-Text (STT) Configuration
STT_INPUT_DIR=./stt_input   # Directory for input audio files
STT_DEFAULT_LANGUAGE=en       # Default language for transcription
STT_MODEL_SIZE=base          # Model size (tiny, base, small, medium, large)

# Image Generation (DALL-E) Configuration
IMAGE_OUTPUT_DIR=./image_output  # Directory for saving generated images
IMAGE_MODEL_VERSION=stability-ai/dall-e  # DALL-E model version
IMAGE_SIZE=1024x1024  # Default image size (1024x1024, 512x512)
IMAGE_QUALITY=standard  # Image quality (standard or hd)
IMAGE_STYLE=vivid  # Image style (vivid or natural)
